{
  "name": "weekly-challenges",
  "version": "1.0.0",
  "description": "MoureDev weekly challenges in javascript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --verbose --watchAll=false",
    "test:2023": "jest --testPathPattern=2023 --verbose",
    "test:watch:2023": "jest --watch --verbose --testPathPattern=2023",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "eslint . --watch"
  },
  "keywords": [
    "javascript",
    "weekly",
    "challenges",
    "algorithms"
  ],
  "author": "Vicente Noriega",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  }
}
